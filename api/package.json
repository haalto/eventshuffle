{
  "name": "events-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "test": "jest \"^(.*/)*(.*\\.)*unit.test.ts$\"",
    "test:all": "docker-compose -f docker-compose.test.yaml up --build",
    "test:watch": "jest --watchAll",
    "migrate:make": "npx knex --knexfile src/knex.ts migrate:make",
    "migrate:latest": "npx knex --knexfile src/knex.ts migrate:latest",
    "migrate:rollback": "npx knex --knexfile src/knex.ts migrate:rollback",
    "seed:make": "npx knex --knexfile src/knex.ts seed:make",
    "seed:run": "npx knex --knexfile src/knex.ts seed:run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "fastify": "^3.25.2",
    "knex": "^0.95.15",
    "pg": "^8.7.1"
  }
}
